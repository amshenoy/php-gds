{
    "name": "tomwalder/php-gds",
    "type": "library",
    "description": "Google Cloud Datastore Library for PHP",
    "keywords": ["google", "datastore", "appengine", "gae", "cloud datastore", "nosql"],
    "homepage": "https://github.com/tomwalder/php-gds",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Tom Walder",
            "email": "tom@docnet.nu"
        }
    ],
    "require": {
        "php": ">=5.4.0"
    },
    "suggest": {
        "google/apiclient": "Allows you to use the JSON API Gateway/Datastore endpoints. Tested with 2.0.0RC2"
    },
    "require-dev": {
        "google/appengine-php-sdk": ">=1.9.22",
        "phpunit/phpunit": "~4.8",
        "satooshi/php-coveralls": "dev-master",
        "google/apiclient": "^2.0.0@RC"
    },
    "autoload": {
        "classmap": [
            "src/"
        ],
        "psr-4": {"GDS\\": "src/GDS/"}
    },
    "include-path": ["src/"]
}
